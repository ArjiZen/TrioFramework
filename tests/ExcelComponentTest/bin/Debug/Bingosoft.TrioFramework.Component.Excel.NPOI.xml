            <param name="collection">表头集合</param>
            <param name="rowIndex">行索引</param>
            <param name="oColumnIndex">当前表头在同级集合中的索引</param>
            <param name="parentAColumnIndex">父级表头在二维数组中的列索引</param>
        </member>
